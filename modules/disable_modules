bin/magento module:disable -f Magento_InventoryGroupedProduct
bin/magento module:disable -f Magento_InventoryGroupedProductAdminUi
bin/magento module:disable -f Magento_InventoryGroupedProductIndexer
bin/magento module:disable -f Magento_InventoryImportExport
