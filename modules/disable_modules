#!/bin/bash

## Description: Disable modules on current project. With command "magento deploy"

bin/magento module:disable Magento_Csp
bin/magento module:disable Magento_TwoFactorAuth

# Disable MSI
bin/magento module:disable Magento_Inventory
bin/magento module:disable Magento_InventoryAdminUi
bin/magento module:disable Magento_InventoryAdvancedCheckout
bin/magento module:disable Magento_InventoryApi
bin/magento module:disable Magento_InventoryBundleProduct
