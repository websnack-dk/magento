bin/magento module:disable -f Magento_InventoryCache
bin/magento module:disable -f Magento_InventorySalesAdminUi
bin/magento module:disable -f Magento_InventorySalesApi
bin/magento module:disable -f Magento_InventorySalesFrontendUi
bin/magento module:disable -f Magento_InventoryShipping
